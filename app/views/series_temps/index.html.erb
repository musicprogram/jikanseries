<p id="notice"><%= notice %></p>

<h1>Series</h1>

<table class="table table-hover" >
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @series_temps.each do |series_temp| %>
      <tr>
        <td><%= series_temp.name %></td>
        <td><%= link_to 'Show', series_temp %></td>
        <td><%= link_to 'Edit', edit_series_temp_path(series_temp) %></td>
        <td><%= link_to 'Destroy', series_temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Series temp', new_series_temp_path %>
